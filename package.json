{
  "name": "express-react-redux-docker-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=6.5",
    "npm": ">=3.10"
  },
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "rimraf dist && NODE_ENV=production node -r babel-register ./node_modules/.bin/webpack -p --config ./webpack/config/production.js --colors -p",
    "build:server": "rimraf build && cp -rf ./src ./build && NODE_ENV=production babel -d ./build ./src/ -s",
    "postinstall": "bash scripts/postinstall.sh",
    "serve:dev": "NODE_ENV=development nodemon --exec babel-node --watch src ./webpack/server.js | pino",
    "serve:prod": "NODE_ENV=production node ./build/server.js"
  },
  "dependencies": {
    "bluebird": "3.5.0",
    "body-parser": "1.17.0",
    "bootable": "^0.2.4",
    "cb": "^0.1.0",
    "express": "4.15.0",
    "express-static-gzip": "^0.2.2",
    "express-webpack-assets": "^0.0.2",
    "helmet": "3.4.1",
    "immutable": "^3.8.1",
    "mongoose": "4.8.5",
    "morgan": "1.8.1",
    "pino": "4.0.2",
    "pug": "^2.0.0-beta11",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-logger": "^2.8.2",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4",
    "server-destroy": "^1.0.1",
    "validator": "^6.2.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-env": "1.2.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "babel-template": "^6.23.0",
    "babel-types": "^6.23.0",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.26.2",
    "enzyme": "^2.7.1",
    "eslint": "3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "6.10.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.10.1",
    "isomorphic-style-loader": "^1.1.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "pixrem": "^3.0.2",
    "pleeease-filters": "^3.0.1",
    "postcss-calc": "^5.3.1",
    "postcss-color-function": "^3.0.0",
